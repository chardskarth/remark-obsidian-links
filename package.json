  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@types/mdast": "^4.0.4",
    "@vitest/coverage-v8": "^3.0.7",
    "@vitest/ui": "^3.0.7",
    "mdsvex": "^0.12.3",
    "rehype-minify-whitespace": "^6.0.2",
    "rehype-parse": "^9.0.1",
    "rehype-stringify": "^10.0.1",
    "to-vfile": "^8.0.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "vitest": "^3.0.7"
