{
  "name": "remark-obsidian-links",
  "version": "1.2.7",
  "description": "a remark plugin that converts obsidian internal links into common links",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
